extends ../../layout.jade
block contenido
 div(class="container")
  h1 Mis Productos
  for producto in productos
   .white.border.tr.medium-padding
     .row
       .col-md-10
         h2=producto.title
       .col-md-2
       a(href="/app/productos/#{producto._id}/edit") Editar
       form(action="/app/productos/#{producto._id}?_method=DELETE",class="Link",method="POST")
        input(type="submit",value="Eliminar")

